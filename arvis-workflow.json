{
  "$schema": "https://raw.githubusercontent.com/jopemachine/arvis-extension-validator/master/workflow-schema.json",
  "creator": "Emmanouil Konstantinidis",
  "description": "Access the React Native Documentation from Arvis",
  "name": "arvis-react-native",
  "readme": "",
  "defaultIcon": "icon.png",
  "version": "0.0.5",
  "webAddress": "https://github.com/jopemachine/arvis-react-native",
  "enabled": true,
  "commands": [
    {
      "type": "scriptFilter",
      "command": "rn",
      "title": "React Native",
      "subtitle": "Access the React Native Documentation",
      "scriptFilter": "node index.js \"{query}\"",
      "runningSubtext": "Loading Documentation",
      "withspace": true,
      "argType": "required",
      "actions": [
        {
          "modifiers": "normal",
          "type": "open",
          "target": "{query}"
        }
      ]
    }
  ]
}
